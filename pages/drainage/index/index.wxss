  background: url('http://ylbb-business.oss-cn-beijing.aliyuncs.com/2020/1586947330863253.png') no-repeat;
  background-size: 100% 100%;
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  top: -98rpx;
  right: 0;
}

.music-icon {
  background: url('http://ylbb-business.oss-cn-beijing.aliyuncs.com/2020/1587021617700145.png') no-repeat;
  background-size: 100% auto;
  width: 80rpx;
  height: 80rpx;
  position: fixed;
  top: 100rpx;
  right: 20rpx;
  z-index: 3;
}

.music-icon.active {
  animation: xzs 3s linear infinite;
  -webkit-animation: xzs 3s linear infinite;
}

@keyframes xzs {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

@-webkit-keyframes xzs {
  0% {
    -webkit-transform: rotate(0deg);
  }

  100% {
    -webkit-transform: rotate(360deg);
  }
}

