.line-scroll {
  width: 750rpx;
  height: 98rpx;
  line-height: 98rpx;
  color: #fff;
  font-size: 36rpx;
  background: #666;
  overflow: hidden;
}

.line-scroll text {
  width: 1750rpx;
  display: block;
  animation: lines 13s linear infinite;
}

@keyframes lines {
  0% {
    transform: translateX(750rpx);
  }

  100% {
    transform: translateX(-100%);
  }
}

